<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">magentando_orderhistoryline_historyline_listing.magentando_orderhistoryline_historyline_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>magentando_orderhistoryline_historyline_columns</spinner>
		<deps>
			<dep>magentando_orderhistoryline_historyline_listing.magentando_orderhistoryline_historyline_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new item</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="magentando_orderhistoryline_historyline_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">historyline_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Magentando_OrderHistoryLine::HistoryLine</aclResource>
		<dataProvider name="magentando_orderhistoryline_historyline_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>historyline_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="magentando_orderhistoryline_historyline_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">magentando_orderhistoryline_historyline_listing.magentando_orderhistoryline_historyline_listing.magentando_orderhistoryline_historyline_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">historyline_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" xsi:type="url" path="magentando_orderhistoryline/HistoryLine/inlineEdit"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">magentando_orderhistoryline_historyline_listing.magentando_orderhistoryline_historyline_listing.magentando_orderhistoryline_historyline_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>historyline_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="historyline_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="description">
			<settings>
				<filter>text</filter>
				<label translate="true">description</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
        <column name="before">
			<settings>
				<filter>text</filter>
				<label translate="true">before</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
        <column name="font_code">
            <settings>
                <filter>text</filter>
                <label translate="true">Icon</label>
                <editor>
                    <editorType>text</editorType>
                    <validation>
                        <rule name="required-entry" xsi:type="boolean">false</rule>
                    </validation>
                </editor>
            </settings>
        </column>
        <column name="status_order">
            <settings>
                <filter>text</filter>
                <label translate="true">Status Order</label>
                <editor>
                    <editorType>text</editorType>
                    <validation>
                        <rule name="required-entry" xsi:type="boolean">false</rule>
                    </validation>
                </editor>
            </settings>
        </column>
		<actionsColumn name="actions" class="Magentando\OrderHistoryLine\Ui\Component\Listing\Column\HistoryLineActions">
			<settings>
				<indexField>historyline_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
	</columns>
</listing>
